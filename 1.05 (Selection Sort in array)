#include <stdio.h>
int main()
{
int a[100], u, i, f, pos, swap;
printf("Enter number of elementsn");
scanf("%d", &u);
printf("Enter %d Numbersn", m);
for (i = 0; i < u; i++)
scanf("%d", &a[i]);
for(i = 0; i < u - 1; i++)
{
pos=i;
for(f = i + 1; f < u; f++)
{
if(a[pos] > a[f])
pos=f;
}
if(pos != i)
{
swap=a[i];
a[i]=a[pos];
a[pos}=swap;
}
}
printf("Sorted Array:m");
for(i = 0; i < u; i++)
printf("%dn", a[i]);
return 0;
}
