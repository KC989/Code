#include <math.h> 
#include <stdio.h> 

//iterates through all the elements and sorts them in ascending order
void insertionSort(int array[], int y) 
{ 
    int s, element, g; 
    for (s = 1; s < y; s++) { element = array[s]; g = s - 1; while (g >= 0 && array[g] > element) { 
            array[g + 1] = array[g]; 
            g = g - 1; 
        } 
        array[g + 1] = element; 
    } 
} 
   //print array elements
void ShowArrayElements(int array[], int y) 
{ 
    int s; 
    for (s = 0; s < y; s++) 
        printf("%d ", ShowArrayElements[s]); 
    printf("n"); 
}
