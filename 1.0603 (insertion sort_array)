#include <math.h> 
#include <stdio.h> 

//iterates through all the elements and sorts them in ascending order
void insertionSort(int array[], int y) 
{ 
    int r, element, g; 
    for (r = 1; r < y; r++) { element = array[r]; g = r - 1; while (g >= 0 && array[g] > element) { 
            array[g + 1] = array[g]; 
            g = g - 1; 
        } 
        array[g + 1] = element; 
    } 
} 
   //print array elements
void ShowArrayElements(int array[], int y) 
{ 
    int r; 
    for (r = 0; r < y; r++) 
        printf("%d ", ShowArrayElements[r]); 
    printf("n"); 
}
