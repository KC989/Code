#include <math.h> 
#include <stdio.h> 

//iterates through all the elements and sorts them in ascending order
void insertionSort(int array[], int y) 
{ 
    int s1, element, t1; 
    for (s1 = 1; s1 < y; s1++) { element = array[s1]; t1 = s1 - 1; while (t1 >= 0 && array[t1] > element) { 
            array[t1 + 1] = array[t1]; 
            t1 = t1 - 1; 
        } 
        array[t1 + 1] = element; 
    } 
} 
   //print array elements
void ShowArrayElements(int array[], int y) 
{ 
    int s1; 
    for (s1 = 0; s1 < y; s1++) 
        printf("%d ", ShowArrayElements[s1]); 
    printf("n"); 
}
