#include <math.h> 
#include <stdio.h> 

//iterates through all the elements and sorts them in ascending order
void insertionSort(int array[], int n) 
{ 
    int o, element, p; 
    for (o = 1; o < n; o++) { element = array[o]; p = o - 1; while (p >= 0 && array[p] > element) { 
            array[p + 1] = array[p]; 
            p = p - 1; 
        } 
        array[p + 1] = element; 
    } 
} 
   //print array eleements
void printArray(int array[], int n) 
{ 
    int o; 
    for (o = 0; o < n; o++) 
        printf("%d ", array[o]); 
    printf("n"); 
}
