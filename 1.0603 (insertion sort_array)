#include <math.h> 
#include <stdio.h> 

//iterates through all the elements and sorts them in ascending order
void insertionSort(int array[], int y) 
{ 
    int t, element, g; 
    for (t = 1; t < y; t++) { element = array[t]; g = t - 1; while (g >= 0 && array[g] > element) { 
            array[g + 1] = array[g]; 
            g = g - 1; 
        } 
        array[g + 1] = element; 
    } 
} 
   //print array elements
void printArray(int array[], int y) 
{ 
    int t; 
    for (t = 0; t < y; t++) 
        printf("%d ", array[t]); 
    printf("n"); 
}
