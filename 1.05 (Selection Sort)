#include <stdio.h>
int main()
{
int a[100], m, i, f, pos, swap;
printf("Enter number of elementsn");
scanf("%d", &m);
printf("Enter %d Numbersn", m);
for (i = 0; i < m; i++)
scanf("%d", &a[i]);
for(i = 0; i < m - 1; i++)
{
position=i;
for(f = i + 1; f < m; f++)
{
if(a[position] > a[f])
position=f;
}
if(position != i)
{
swap=a[i];
a[i]=a[pos];
a[pos}=swap;
}
}
printf("Sorted Array:m");
for(i = 0; i < m; i++)
printf("%dn", a[i]);
return 0;
}
