#include <stdlib.h>

int * generateArray(int n){
int *t = malloc ( n * sizeof(int) );
if(t){
for( i = 0, i< n, i++){
a[i] = i;
}
return t;
}

void printArray(int *a, int n){
for( int i = 0, i<n; i++)
{
printf(" %d ", a[i]); 
}

void printArray(int *a, int n){
printf("Array = ");
for( int i = 0, i<n; i++)
{
printf(" %d ", a[i]); 
}
printf("\n");
}

int main(int argc, char **argv){
int * a;
int n = 10;

a = generateArray(n);

free(a);
return 0;
}
