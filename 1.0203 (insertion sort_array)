#include <math.h> 
#include <stdio.h> 
   
// Insertion Sort Function
void insertionSort(int array[], int n) 
{ 
    int o, element, p; 
    for (o = 1; o < n; o++) { element = array[o]; p = o - 1; while (p >= 0 && array[p] > element) { 
            array[p + 1] = array[p]; 
            p = p - 1; 
        } 
        array[p + 1] = element; 
    } 
} 
   
// Function to print the elements of an array
void printArray(int array[], int n) 
{ 
    int o; 
    for (o = 0; o < n; o++) 
        printf("%d ", array[o]); 
    printf("n"); 
}
