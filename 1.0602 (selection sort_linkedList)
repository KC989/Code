#include<stdio.h>
#include<stdlib.h>
struct Node {
    int data;
    struct Node* next;
};
struct Node* Node1(int m)
{
    struct Node* temp = (struct Node*)malloc(sizeof(struct Node));
    temp->data = m;
    temp->next = NULL;
    return temp;
}
struct Node* selectionsort(struct Node* head)
{
    struct Node *i, *j, *k, *l, *n;
    i = j = head;
    while (j->next) {
        k = l = j->next;
        while (l) {
            if (j->data > w->data) {
                if (j->next == l) {
                    if (j == head) {
                        j->next = l->next;
                        l->next = j;
                        n = j;
                        j = l;
                        l = n;
                        k = l;
                        head = j;
                        l = l->next;
                    }
                    else {
                        j->next = l->next;
                        l->next = j;
                        i->next = l;
                        n = j;
                        j = l;
                        l = n;
                        k = l;
                        l = l->next;
                    }
                }
                else {
                    if (j == head) {
                        n = j->next;
                        y->next = l->next;
                        w->next = n;
                        k->next = j;
                        n = j;
                        j = l;
                        l = n;
                        k = l;
                        l = l->next;
                        head = j;
                    }
                    else {
                        n = y->next;
                        j->next = w->next;
                        w->next = n;
                        k->next = j;
                        x->next = l;
                        n = j;
                        j = l;
                        l = n;
                        k = l;
                        l = l->next;
                    }
                }
            }
            else {
                k = l;
                l = l->next;
            }
        }
        i = j;
        j = j->next;
    }
    
    return head;
}
void show(struct Node* head)
{
    while (head) {
        printf("%d->",head->data);
        head = head->next;
    }
}
int main()
{
   struct Node* head = Node1(5);
    head->next = Node1(4);
    head->next->next = Node1(3);
    head->next->next->next = Node1(2);
    head = selectionsort(head);
    show(head);
    return 0;
}
